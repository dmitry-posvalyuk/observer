          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\r\n  number,\r\n  \"started at\",\r\n  \"order processing time\",\r\n  CASE\r\n    WHEN \"diff from average in hour\" < 90 THEN 'shipment is not finished'\r\n    WHEN \"diff from average in hour\" < 100 THEN 'failed to send'\r\n    WHEN \"diff from average in hour\" >= 100 THEN 'not ready to collect'\r\n    END AS reason\r\nFROM (\r\n  SELECT\r\n    number,\r\n    \"createdAt\" AS \"started at\",\r\n    \"completedAt\" - \"createdAt\" AS \"order processing time\",\r\n    EXTRACT(epoch FROM \"completedAt\" - \"createdAt\" - '${orderAverageTime}')/3600 AS \"diff from average in hour\"\r\n  FROM orders \r\n  WHERE $__timeFilter(\"createdAt\") AND orders.\"completedAt\" IS NOT NULL\r\n)\r\nWHERE \"diff from average in hour\" > 80",
