          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \r\n  #*\r\n  SUBSTRING(SEC_TO_TIME(AVG(CASE WHEN type = 'confirmed' THEN TIME_TO_SEC(`diff`) END)), 1, 8) as 'confirmed -> payment-pending',\r\n  SUBSTRING(SEC_TO_TIME(AVG(CASE WHEN type = 'payment-pending' THEN TIME_TO_SEC(`diff`) END)), 1, 8) as 'payment-pending -> dispatched',\r\n  SUBSTRING(SEC_TO_TIME(AVG(CASE WHEN type = 'dispatched' THEN TIME_TO_SEC(`diff`) END)), 1, 8) as 'dispatched -> collected'\r\nFROM (\r\n  SELECT *, \r\n    SEC_TO_TIME(TIMESTAMPDIFF(SECOND, createdAt, LEAD(createdAt) OVER (ORDER BY orderNumber))) AS diff\r\n  FROM (\r\n    SELECT * FROM events \r\n    WHERE $__timeFilter(createdAt) AND type IN ('confirmed', 'payment-pending', 'dispatched', 'collected') \r\n    ORDER BY orderNumber LIMIT 10) as events\r\n) as events\r\nWHERE type IN ('confirmed', 'payment-pending', 'dispatched')\r\n#GROUP BY type",
          "refId": "A",
